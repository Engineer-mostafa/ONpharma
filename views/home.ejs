            <!--Patient Form Regesteration-->
            <!--------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
            <form id="registrationFormPatient" class="form-validate" method="POST">

                <div class="modal fade" id="modal-1" tabindex="-1" role="modal" aria-labelledby="modal-label-3"
                    aria-hidden="true">
                    <div class="modal-dialog modal-md modal-dialog-centered">
                        <div class="modal-content">
                            <div class="modal-header">

                                <h4 id="modal-label-3" class="modal-title">Register As A Patient</h4>
                                <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                            </div>
                            <div class="modal-body">
                                <div class="form-row">

                                    <div class="form-group col-md-6">
                                        <label for="name">First Name<span class="text-danger">*</span></label>
                                        <input type="text" id="name" class="form-control" name="Fname"
                                            placeholder="Enter First name" required>

                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="name">Last Name<span class="text-danger">*</span></label>
                                        <input type="text" id="name" class="form-control" name="Lname"
                                            placeholder="Enter Last name" required>

                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-md-6">
                                        <label for="email">Email address<span class="text-danger">*</span></label>
                                        <input type="email" id="email" class="form-control" name="email"
                                            placeholder="Enter your email" required>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="mobile">Mobile<span class="text-danger">*</span></label>
                                        <input class="form-control" id="mobile" type="number" name="mobile"
                                            placeholder="Enter your Mobile number" required>
                                    </div>
                                </div>
                                <div class="form-row">
                                    <div class="form-group col-md-6">
                                        <label for="gender">Gender<span class="text-danger">*</span></label>
                                        <select class="form-control" id="gender" name="gender" required>
                                            <option value= -1>Select your gender</option>
                                            <option value= 0>Male</option>
                                            <option value= 1>Female</option>
                                        </select>
                                    </div>

                                </div>

                                <div class="form-row">
                                    <div class="form-group col-md-12">
                                        <div class="g-recaptcha"
                                            data-sitekey="6LeyKeMZAAAAAE_UxpP8OwAT51JFUMaFVrbZ5vXF">
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="modal-footer">
                                <button data-dismiss="modal" class="btn btn-b" type="button">Close</button>
                                <button class="btn btn-b" type="button">Sign Up</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>







